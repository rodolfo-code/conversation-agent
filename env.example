# Configurações OpenAI (OBRIGATÓRIO)
OPENAI_API_KEY=sua_chave_api_openai_aqui

# Configurações do Modelo OpenAI (OPCIONAL)
OPENAI_MODEL_NAME=gpt-4o-mini
OPENAI_TEMPERATURE=0.2

# Configurações LangChain Tracing V2
# IMPORTANTE: LANGCHAIN_TRACING_V2 deve ser "true" ou "false" (string)
# Se "true", você DEVE fornecer LANGCHAIN_API_KEY válida
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Configurações de Log (OPCIONAL)
LOG_LEVEL=INFO
ENVIRONMENT=development

# NOTA IMPORTANTE: 
# - LANGCHAIN_TRACING_V2=false: LangSmith desabilitado (recomendado para desenvolvimento)
# - LANGCHAIN_TRACING_V2=true: LangSmith habilitado (requer LANGCHAIN_API_KEY válida)
# - A versão antiga (LANGCHAIN_TRACING) é automaticamente desabilitada
